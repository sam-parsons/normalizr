{
    "manifest_version": 3,
    "name": "normalizr",
    "version": "1.0",
    "description": "Normalizes audio in real-time on web pages using the Web Audio API.",
    "permissions": [
      "tabCapture", 
      "activeTab"
    ],
    "background": {
      "service_worker": "dist/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
  